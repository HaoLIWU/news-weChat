/* pages/collect/collect.wxss */
page{
  background-color: #FFFFF0;
}
.newsBox{
  width: 100%;
  height: 230rpx;
  background-color: #fff;
  position: relative;
  padding: 28rpx 20rpx;
  border-bottom: 2rpx solid #333;
}
.newsBox-img{
  width: 276rpx;
  height: 230rpx;
  float: right;
  margin-right: 45rpx;
}
.newsBox-title{
  width: 400rpx;
  float: left;
}
.newBox-time{
  font-size: 16px;
  position: absolute;
  bottom: 28rpx;
}